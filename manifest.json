{
  "manifest_version": 2,
  "name": "User-Agent Switcher",
  "version": "1.2",
  "description": "Switches User-Agent string to mimic Chrome Android 13 on a Pixel 5 device.",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background/background.js"]
  },
  "browser_action": {
    "default_title": "User-Agent Switcher"
  }
}
